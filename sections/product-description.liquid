                              :class="{ 'tab_active': activeTab === 'product_review' }"
                        >
                        {% if block.settings.review_tab != blank %}
                        <div class="cursor-pointer">
                              {{ block.settings.review_tab}}
                        </div>
                        {% else %}
                              <div class="cursor-pointer"> {{ block.settings.review_tab}} </div>
                        {% endif %}
