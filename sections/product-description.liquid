<style>
.tab_buttons{
      background-color: {{section.settings.color_button}};
      color:{{section.settings.button_font_color}}; 
      border-bottom: 1px solid {{section.settings.button_border_color}};
}
.tab_buttons:hover{
      background-color: {{section.settings.button_color_hover}};
      color:{{section.settings.button_font_hover}};
      border-bottom: 1px solid {{section.settings.button_border_hover}};
}
.tab_active{
  background-color: {{section.settings.active_button_color }};
  color:{{section.settings.active_font_color }};
  border-bottom: 1px solid {{section.settings.active_border_color}};
}

.tab-panel{
      border: none;
}

.spr-container{
      background-color: #fafafa;
      color: #4b4b4b;
      border-radius: 5px;
}
  
  @media(max-width: 767px){
   .mobile-border{
    border:none !important;
    }   
  }
</style>


<div class="{{section.settings.color_schema}}">
      <div class="xl:max-w-[1320px] m-auto overflow-hidden px-4 md:px-8" 
      style="padding-top:{{section.settings.padding_top}}rem; padding-bottom:{{section.settings.padding_bottom}}rem;"
      {% if section.settings.animation_setting == true %} data-aos="fade-up" data-aos-duration="3000" {% endif %}>

        {%- comment -%} headline {%- endcomment -%}
            <div class="md:mx-[1rem] mb-[1.5rem] flex flex-col {{section.settings.text_align}}">
                <div class="Left_titlebox">
                  {% if section.settings.subtitle != blank %}
                      <span class="text-xs my-9 leading-8 tracking-wider" style="color:{{section.settings.color_text_title}};">
                        {{section.settings.subtitle}}
                      </span>
                    {% endif %}
                    {% if section.settings.headline != blank %}
                      <h1 class="mb-[1.2rem] text-2xl lg:text-3xl font-bold tracking-wider" style="color:{{section.settings.color_title}};">
                        {{ section.settings.headline }}
                      </h1>
                    {% endif %}
                      <div class="subfont_size my-8 leading-8 tracking-wider" style="color:{{section.settings.color_text_title}};">
                        {{ section.settings.text }}
                       </div>  
                  </div>
            </div>

            <div class="tab-wrapper" x-data="{ activeTab: 'about_product' }">    
                  
                  <div id="tab_selector" class="flex flex-wrap {{ section.settings.collection_button_align }} mb-6">
                  {%- comment -%} Tab Selector {%- endcomment -%}
                  {% for block in section.blocks %}
                  {% case block.type %}
                  
                  {% when 'about_product' %}
                        <label
                              x-on:click = "activeTab = 'about_product' "
                              class="min-w-[33%] tab-control px-3 py-2 mx-2 tracking-widest mx-2 tab_buttons text-center outline-none transition duration-100 {{section.settings.collection_button_size}} {% if section.settings.button_round_off == true %} button_round {% endif %}"
                              :class="{ 'tab_active': activeTab === 'about_product'}"
                        >
                        {% if block.settings.headline_tab_title != blank %}
                        <div class="cursor-pointer">
                              {{ block.settings.headline_tab_title}}
                        </div>
                        {% else %}
                              <div class="cursor-pointer"> {{ block.settings.headline_tab_title}} </div>
                        {% endif %}
                        </label>

                  {% when 'product_review' %}
                        <label
                              x-on:click="activeTab = 'product_review' "
