/* IMAGE ANIMATION */

@keyframes zoominoutsinglefeatured {
      0% {
          transform: scale(1,1);
      }
      50% {
          transform: scale(1.2,1.2);
      }
      100% {
          transform: scale(1,1);
      }
  }
  
  .cardcontainer img {
      animation: zoominoutsinglefeatured 5s ;
  }


