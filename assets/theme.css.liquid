/* FONT STYLES */

* {
      font-family: {{ settings.base_font_new.family }}, {{ settings.base_font_new.fallback_families }};
}

h1 {
      font-family: {{ settings.header_font_new.family }}, {{ settings.header_font_new.fallback_families }};
      font-size: {{ settings.header_base_size }}px !important;
}

.subfont_size{
      font-size: {{ settings.subfont_size }}px !important;
}

.slideshow_header{
      font-family: {{ settings.slideshow_header.family }}, {{ settings.slideshow_header.fallback_families }};

}

.hero_header{
      font-family: {{ settings.hero_header.family }}, {{ settings.hero_header.fallback_families }};
      font-size: {{ settings.banner_font_size }}px !important;
