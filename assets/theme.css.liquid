/* quantity style */

.qty_control label{display: block;margin-bottom: 12px;letter-spacing: 2.8px;color: #747a7b;}
.qty_control .btnqty{display: inline-block;cursor: pointer;user-select: none;font-size: 25px;padding: 5px;line-height: 5px;}
.qty_control .btnqty.qtyminus{margin-right: 8px;}
.qty_control .btnqty.qtyplus{margin-left: 8px;}
.qty_control .quantity-input{border: none;border: none;padding: 8px;text-align: center;width: 50px;outline: none;display: inline-block;}
.qty_control {display: inline-block;}


/* news letter style */

.image_hide{
      flex-direction: column;
      display: none;
      width: 50%;
}

/* PRICE FILTER STYLE */
.price-input{
      width: 100%;
      display: flex;
      margin: 30px 0 35px;
}


.separator{
      display: flex;
      align-items: center;
      justify-content: center;
      position: relative;
      top: 11px;
      margin: 8px;
}

input[type="number"]::-webkit-outer-spin-button,
input[type="number"]::-webkit-inner-spin-button{
      -webkit-appearance: none;
      -moz-appearance: textfield;
}

.price-slider{
      position: relative;
      width: 100%;
      height: 5px;
      border-radius: 5px;
      background: #ddd;
}

.price-slider .progress{
      height: 5px;
      

}

.range-input{
      position: relative;
}

.range-input input{
      position: absolute;
      top: -5px;
      height: 5px !important;
      width: 100%;
      background: none;
      pointer-events: none;
      -webkit-appearance: none;
      padding: 0;
      border: none;
}

