/*.gallery*/
.product_box{
	width: 100%;
      margin-top:5rem;
	
}
.product_box .gallery-filter{
	padding: 0 15px;
	width: 100%;
	text-align: center;
	margin-bottom: 20px;
}
.product_box .gallery-filter .filter-item{
	color: #4b4b4b;
	font-size: 18px;
	text-transform: uppercase;
	display: inline-block;
	margin:0 10px;
	cursor: pointer;
	border-bottom: 2px solid transparent;
	line-height: 1.2;
	transition: all 0.3s ease;
}
.product_box .gallery-filter .filter-item.active{
	color: #ff9800;
	border-color : #ff9800;
}

.product_box .gallery-item-inner img{
	width: 100%;
}
.product_box .gallery-item.show{
	animation: fadeIn 0.5s ease;
}

.gallery-h{
  height:400px;
}

.gallery-modal{
  position:relative;
  z-index:100;
  top:-150%;
}

.modal{
  top:-50%;
}
.email_form{
  gap:1rem;
  align-items:end;
}

@keyframes fadeIn{
	0%{
		opacity: 0;
	}
	100%{
		opacity: 1;
	}
}
.product_box .gallery-item.hide{
	display: none;
}

/*responsive*/

@media(max-width: 767px){
    .product_box .gallery-item{
		width: 100%;
	}	
	.product_box .gallery-filter .filter-item{
